@model GyanmitrasMDL.User.StudentMDL
@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<style>
    .field-validation-error {
        color: red;
    }

    .field-icon {
        float: right;
        margin-left: -25px;
        margin-top: -25px;
        position: relative;
        z-index: 2;
    }
</style>
@using (Html.BeginForm("AddEditUser", "ManageResources", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.Partial("../../Views/Student/_StudentRegistration", (GyanmitrasMDL.User.StudentMDL)ViewBag.Registration)

}

@section scripts {\
    @Scripts.Render("~/Admin/bundles/jqueryval")
    <script src="~/app-assets/vendors/js/forms/select/select2.min.js"></script>
    <script src="~/assets/js/jquery.sumoselect.js"></script>
  

}