                                    </div>
                                </div>
                            </div>*@
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>


@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        $(function () {

            $(".card").click(function () {
                if ($(this).find('table > tbody > tr').length <= 0) {
                    $('.card').removeAttr('style');
                    $(this).attr('style', 'background:#c5d6c5');
                }
            });
            $("#AdminFeedSectionDev").trigger('click');
            $("#AdminFeedSectionDev").find('.card').trigger('click');

            
        });
        
        

    </script>
}