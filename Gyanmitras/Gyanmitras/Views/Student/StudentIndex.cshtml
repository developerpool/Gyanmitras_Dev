            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-12 col-md-12 offset-lg-1">
                        <div class="form-group row">

                            <label class="col-md-3 control-label text-center" for="eventRegInput1"></label>
                            <div class="col-md-9">
                                <input type="checkbox" id="chk1" name="chk1" value="Bike">
                                <label for="chk1">Did your counselor suggest any private institution?</label><br>
                                <input type="checkbox" id="chk2" name="chk2" value="Bike">
                                <label for="chk2">Were u asked to pay money for guidance?</label><br>
                                <input type="checkbox" id="chk3" name="chk3" value="Bike">
                                <label for="chk3">Was your counselor in hurry?</label><br>
                                <input type="checkbox" id="chk4" name="chk4" value="Bike">
                                <label for="chk4"> Were you asked for any other unregistered contact?</label><br>
                                <input type="checkbox" id="chk5" name="chk5" value="Bike">
                                <label for="chk5"> Was your counselor on time?</label><br>
                                <input type="checkbox" id="chk6" name="chk6" value="Bike">
                                <label for="chk6"> Did your counselor lie?</label><br>
                                <input type="checkbox" id="chk7" name="chk7" value="Bike">
                                <label for="chk7">Was your counselor inefficient? If yes ->Explain</label><br>
                                <textarea id="txtareainefficient" class="form-control" maxlength="200"></textarea><br>
                                <label>Report Abuse -></label><br>
                                <label>Explain your concern:</label><br>
                                <textarea id="txtareaabuseconcern" class="form-control" maxlength="200" placeholder="explain your concern"></textarea><br>
                                <div class="custom-file">
                                    <label>Submit docs if any ,Related to abuse:</label><br>
                                    <input type="file" id="filedoc" class="form-control" onchange="document.getElementById('blah').src = window.URL.createObjectURL(this.files[0])" />

                                    Your Image Should be in (jpg, jpeg, png) format.
                                </div>
                            </div>

                        </div>

                        <div class="form-actions center">

                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <a href="Javascript:Void();" class="btn btn-primary box-shadow-1 round btn-min-width mr-1 mb-1" id="btnsubmitfeedback">Submit</a>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>

            </div>
        </div>
    </div>
</div>

@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/assets/js/jquery.sumoselect.js"></script>
    <link href="~/app-assets/vendors/css/forms/selects/select2.min.css" rel="stylesheet" />
    <script src="~/app-assets/vendors/js/forms/select/select2.min.js"></script>



    <style>
        .field-validation-error, .red {
            color: red;
        }
    </style>
    <script type="text/javascript">
        var mycategoryval = new Array();
        var mycategorytext = new Array();
        $(document).ready(function () {


            BindAreaOfInterest("counselor");
            StateBind();
            EducationType();
           // EducationSubCategoryType();
           // StreamType();
           // BoardType();
            $(".select2").select2();


            $("button[type='submit']").click(function (e) {
                alert('Comming Soon!');
                return false;
            });

            $("#ddlissue").change(function () {
                if ($("#ddlissue").val() == "Others") {
                    $('#divtxtarea').show();
                }
                else {
                    $('#divtxtarea').hide();
                }
