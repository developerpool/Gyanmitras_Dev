                                            @Html.ValidationMessageFor(E => E.BoardType)
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group row" id="divlessthan10" style="display:none">
                                    <label class="col-md-3 control-label" for="eventRegInput1">Nature of Completion <span class="red">*</span></label>
