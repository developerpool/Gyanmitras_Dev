                    <div class="row">
                        <div class="col-lg-12 offset-lg-1">
                            <div class="form-body">
                                <div class="row">
                                    <div class="col-12 mt-1 mb-2">
                                        <h4>Details</h4>
                                        <hr>
                                    </div>
                                </div>

